<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.quali.confirmation.model.vo.Confirmation" alias="confirmation" />
		<typeAlias type="com.kh.quali.member.model.vo.Member" alias="member"/>
		<typeAlias type="com.kh.quali.member.model.vo.EducationStatus" alias="educationStatus"/>
		<typeAlias type="com.kh.quali.notice.model.vo.Notice" alias="notice"/>
	</typeAliases>

	<mappers>
		<mapper resource="/mapper/member-mapper.xml" />
		<mapper resource="/mapper/qualification-mapper.xml" />
		<mapper resource="/mapper/notice-mapper.xml" />
	</mappers>



</configuration>